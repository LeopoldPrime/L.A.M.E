                <Button x:Name="RotationsNavbar" FontFamily="Segoe UI Light" Content="Damage Rotations" HorizontalContentAlignment="Left" Style="{StaticResource NavbarStyleOne}" Foreground="{DynamicResource IanactiveForground}" Canvas.Left="10" Canvas.Top="53" Width="151" Height="23" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="12" Click="NavbarButtonClicked"/>
